pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                sh '''
                   git clone https://github.com/thangacodes/dotnet_webapp.git
              '''
            }
        }
        stage('Build') {
            steps {
                sh '''
                   cd DotNet_project/Infrabuild_scripts
                   ls -l 
                   terraform init
                   terraform validate
                   terraform plan -var-file terraform.tfvars
                   '''
            }
        }
    }
}
