pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                sh '''
                   git clone https://github.com/thangacodes/dotnet_webapp.git
              '''
            }
        }
        stage('Build') {
            steps {
                   sh 'cd Infra_build/Infrabuild_scripts/'
                   sh '''
                      ls -l 
                      terraform init
                      terraform validate
                      terraform plan -var-file terraform.tfvars
                   '''
            }
        }
    }
}
